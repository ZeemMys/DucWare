-- H√†m t·∫°o Fake Kick UI
local function createFakeKick(reason)
    if LocalPlayer:FindFirstChild("PlayerGui"):FindFirstChild("FakeKick") then
        LocalPlayer.PlayerGui:FindFirstChild("FakeKick"):Destroy()
    end

    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "FakeKick"
    screenGui.ResetOnSpawn = false
    screenGui.Parent = LocalPlayer:WaitForChild("PlayerGui")

    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(0, 500, 0, 200)
    frame.Position = UDim2.new(0.5, -250, 0.5, -100)
    frame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
    frame.BorderSizePixel = 0
    frame.Parent = screenGui

    Instance.new("UICorner", frame).CornerRadius = UDim.new(0, 6)

    local title = Instance.new("TextLabel")
    title.Text = "Disconnected"
    title.Font = Enum.Font.SourceSansBold
    title.TextSize = 28
    title.TextColor3 = Color3.fromRGB(255, 255, 255)
    title.BackgroundTransparency = 1
    title.Size = UDim2.new(1, 0, 0, 50)
    title.Position = UDim2.new(0, 0, 0, 10)
    title.Parent = frame

    local message = Instance.new("TextLabel")
    message.Text = "You have been kicked by this experience or its moderators. Moderation message:\n\n" .. reason .. "\n(Error Code: 267)"
    message.Font = Enum.Font.SourceSans
    message.TextSize = 20
    message.TextColor3 = Color3.fromRGB(220, 220, 220)
    message.TextWrapped = true
    message.TextYAlignment = Enum.TextYAlignment.Top
    message.BackgroundTransparency = 1
    message.Size = UDim2.new(1, -40, 0, 100)
    message.Position = UDim2.new(0, 20, 0, 60)
    message.Parent = frame

    local button = Instance.new("TextButton")
    button.Text = "Leave"
    button.Font = Enum.Font.SourceSansBold
    button.TextSize = 22
    button.TextColor3 = Color3.fromRGB(255, 255, 255)
    button.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
    button.Size = UDim2.new(0, 100, 0, 40)
    button.Position = UDim2.new(0.5, -50, 1, -60)
    button.Parent = frame

    Instance.new("UICorner", button).CornerRadius = UDim.new(0, 6)

    button.MouseButton1Click:Connect(function()
        screenGui:Destroy()
    end)
end

-- N√∫t troll v√†o tab Other
Tabs.Other:AddButton({
    Title = "üíÄ Press To See )",
    Description = "ngu=)))",
    Callback = function()
        createFakeKick("ngu vai lon")
    end
