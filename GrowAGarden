GetKey.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
GetKey.Parent = Frame

-- ğŸ“ Link Get Key
local getKeyLink = "https://link-center.net/1351480/get-key-system"

-- ğŸ” Key xÃ¡c thá»±c
local correctKey = "FreeKeyScrgarden"

-- ğŸ–±ï¸ NÃºt Get Key
GetKey.MouseButton1Click:Connect(function()
    setclipboard(getKeyLink)
    GetKey.Text = "âœ… Copied!"
    task.wait(1)
    GetKey.Text = "ğŸ”— Get Key"
end)

-- ğŸ–±ï¸ Khi áº¥n nÃºt Submit
Submit.MouseButton1Click:Connect(function()
	if TextBox.Text == correctKey then
		ScreenGui:Destroy()

		-- âœ… Náº¿u Ä‘Ãºng key, load GUI chÃ­nh
		loadstring(game:HttpGet("https://raw.githubusercontent.com/ZeemMys/DucWare/main/hahahaha"))()
