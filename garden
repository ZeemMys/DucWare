Tabs.Other:AddButton({
    Title = "üíÄ",
    Description = "Ch·∫°m ƒë·ªÉ bi·∫øt:)",
    Callback = function()
        local Players = game:GetService("Players")
        local LocalPlayer = Players.LocalPlayer

        -- T·∫°o GUI
        local screen = Instance.new("ScreenGui")
        screen.Name = "FakeKickMessage"
        screen.ResetOnSpawn = false
        screen.IgnoreGuiInset = true
        screen.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
        screen.Parent = LocalPlayer:WaitForChild("PlayerGui")

        -- L√†m m·ªù n·ªÅn
        local blur = Instance.new("Frame", screen)
        blur.Size = UDim2.new(1, 0, 1, 0)
        blur.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        blur.BackgroundTransparency = 0.3

        -- Frame ch√≠nh
        local frame = Instance.new("Frame", screen)
        frame.Position = UDim2.new(0.5, 0, 0.5, 0)
        frame.AnchorPoint = Vector2.new(0.5, 0.5)
        frame.Size = UDim2.new(0, 420, 0, 180)
        frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)

        -- Bo tr√≤n
        local corner = Instance.new("UICorner", frame)
        corner.CornerRadius = UDim.new(0, 8)

        -- Vi·ªÅn s√°ng
        local stroke = Instance.new("UIStroke", frame)
        stroke.Color = Color3.fromRGB(255, 50, 50)
        stroke.Thickness = 2

        -- Ti√™u ƒë·ªÅ
        local title = Instance.new("TextLabel", frame)
        title.Text = "Disconnected"
        title.Size = UDim2.new(1, 0, 0.25, 0)
        title.Position = UDim2.new(0, 0, 0, 10)
        title.BackgroundTransparency = 1
        title.Font = Enum.Font.GothamBold
        title.TextColor3 = Color3.fromRGB(255, 70, 70)
        title.TextScaled = true

        -- N·ªôi dung
        local content = Instance.new("TextLabel", frame)
        content.Text = "You have been kicked from this experience.\n(Error Code: 267)"
        content.Size = UDim2.new(1, -40, 0.45, 0)
        content.Position = UDim2.new(0, 20, 0.3, 0)
        content.BackgroundTransparency = 1
        content.TextColor3 = Color3.fromRGB(230, 230, 230)
        content.Font = Enum.Font.Gotham
        content.TextWrapped = true
        content.TextYAlignment = Enum.TextYAlignment.Top
        content.TextScaled = true

        -- N√∫t OK
        local ok = Instance.new("TextButton", frame)
        ok.Text = "OK"
        ok.Size = UDim2.new(0, 100, 0, 36)
        ok.Position = UDim2.new(0.5, -50, 1, -50)
        ok.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
        ok.TextColor3 = Color3.new(1, 1, 1)
        ok.Font = Enum.Font.GothamMedium
        ok.TextSize = 18

        -- Bo tr√≤n n√∫t
        local okCorner = Instance.new("UICorner", ok)
        okCorner.CornerRadius = UDim.new(0, 6)

        -- T√°c v·ª•
        ok.MouseButton1Click:Connect(function()
            screen:Destroy()
        end)
    end
})
